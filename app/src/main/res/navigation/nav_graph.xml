<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/logInFragment">

    <fragment
        android:id="@+id/logInFragment"
        android:name="com.example.abschlussaufgabe.ui.LogInFragment"
        android:label="fragment_log_in"
        tools:layout="@layout/fragment_log_in" >
        <action
            android:id="@+id/action_logInFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.abschlussaufgabe.ui.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home"/>
    <fragment
        android:id="@+id/carFragment"
        android:name="com.example.abschlussaufgabe.ui.CarFragment"
        android:label="fragment_car"
        tools:layout="@layout/fragment_car" />
    <fragment
        android:id="@+id/materialFragment"
        android:name="com.example.abschlussaufgabe.ui.MaterialFragment"
        android:label="fragment_material"
        tools:layout="@layout/fragment_material" >
        <argument
            android:name="user_id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/workTimeFragment"
        android:name="com.example.abschlussaufgabe.ui.WorkTimeFragment"
        android:label="fragment_work_time"
        tools:layout="@layout/fragment_work_time" />
    <fragment
        android:id="@+id/materialReceivedFragment"
        android:name="com.example.abschlussaufgabe.ui.MaterialReceivedFragment"
        android:label="MaterialReceivedFragment" />
    <fragment
        android:id="@+id/materialDeliverFragment"
        android:name="com.example.abschlussaufgabe.ui.MaterialDeliverFragment"
        android:label="MaterialDeliverFragment" />
    <fragment
        android:id="@+id/materialPlacementFragment"
        android:name="com.example.abschlussaufgabe.ui.MaterialPlacementFragment"
        android:label="MaterialPlacementFragment" />
    <fragment
        android:id="@+id/carGetInFragment"
        android:name="com.example.abschlussaufgabe.ui.CarGetInFragment"
        android:label="CarGetInFragment" />
</navigation>